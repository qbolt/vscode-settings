        },
        {
            "before": [
                "<leader>",
                "r",
                "f"
            ],
            "commands": [
                "fileutils.renameFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "e"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "o",
                "g"
            ],
            "commands": [
                "workbench.action.showAllSymbols"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "lineBreakInsert"
            ],
            "silent": true
        }
    ],
    "vim.smartRelativeLine": true,
    "vim.useSystemClipboard": true,
    "dateTime.showMonth": true,
    "dateTime.showDayOfMonth": true,
    "dateTime.showDayOfWeek": true,
    "window.titleBarStyle": "custom",
    "workbench.iconTheme": "file-icons",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.productIconTheme": "fluent-icons",
    "editor.accessibilitySupport": "off",
    "workbench.colorTheme": "Field Lights",
    "cSpell.userWords": [
        "Pickture"
    ],
    "window.zoomLevel": 1,
    "zenMode.showTabs": "single",
    "godotTools.editorPath.godot4": "C:/Users/qbolt/Godot_v4.2.1-stable_mono_win64/Godot_v4.2.1-stable_mono_win64.exe"
